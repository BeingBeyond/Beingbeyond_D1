                                "setup_only": true
                            }
                        },
                        "rigid_shape_properties": {
                            "friction": {
                                "num_buckets": 250,
                                "range": [
                                    0.7,
                                    1.3
                                ],
                                "operation": "scaling",
                                "distribution": "uniform"
                            }
                        }
                    },
                    "object": {
                        "scale": {
                            "range": [
                                0.95,
                                1.05
                            ],
                            "operation": "scaling",
